            <declare name="rDist" type="Real" array="False" size=""/>
            <assign variable="rDist" expression="((rVelin+rVelfin)/2)*t"/>
            <output expression="&quot;La distancia es: &quot;&amp;rDist" newline="True"/>
        </body>
