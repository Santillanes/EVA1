    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <output expression="&quot;Introduce la velocidad inicial&quot;" newline="True"/>
            <declare name="rVelin" type="Real" array="False" size=""/>
            <input variable="rVelin"/>
            <output expression="&quot;Introduce la velocidad final&quot;" newline="True"/>
            <declare name="rVelfin" type="Real" array="False" size=""/>
            <input variable="rVelfin"/>
            <output expression="&quot;Introduce el tiempo empleado&quot;" newline="True"/>
            <declare name="t" type="Real" array="False" size=""/>
